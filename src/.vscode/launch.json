    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/Estimator.out",
            "args": ["--name=out.txt", "--n=10", "--noise=false", "--norm=1", "--axis=1,0,0", "--angle=3.141592653589793"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "logging": {
                "trace": true,
                "traceResponse": true,
                "engineLogging": true
            },
            "preLaunchTask": "Build with Clang"
        }
    ]
}